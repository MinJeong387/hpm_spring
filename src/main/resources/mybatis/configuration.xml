<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
	
<configuration>
	<typeAliases>
		<typeAlias alias="UserVo" type="himedia.hpm_spring.repository.vo.UserVo" />
		<typeAlias alias="CommunityVo" type="himedia.hpm_spring.repository.vo.CommunityVo" />
		<typeAlias alias="mReviewVo" type="himedia.hpm_spring.repository.vo.MountainReviewVo"/>
		<typeAlias alias="rReviewVo" type="himedia.hpm_spring.repository.vo.RestaurantReviewVo"/>
		<typeAlias alias="UserPhotoVo" type="himedia.hpm_spring.repository.vo.UserPhotoVo" />
		<typeAlias alias="CommunityPhotoVo" type="himedia.hpm_spring.repository.vo.CommunityPhotoVo" />
		<typeAlias alias="RestaurantPhotoVo" type="himedia.hpm_spring.repository.vo.RestaurantPhotoVo" />
		<typeAlias alias="MountainPhotoVo" type="himedia.hpm_spring.repository.vo.MountainPhotoVo" />
		<typeAlias alias="ReviewPhotoVo" type="himedia.hpm_spring.repository.vo.ReviewPhotoVo" />
		<typeAlias alias="cCommentVo" type="himedia.hpm_spring.repository.vo.CommunityCommentVo"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/mappers/user.xml" />
		<mapper resource="mybatis/mappers/community.xml" />
		<mapper resource="mybatis/mappers/mountainReview.xml" />
		<mapper resource="mybatis/mappers/restaurantReview.xml" />
		<mapper resource="mybatis/mappers/userPhoto.xml" />
		<mapper resource="mybatis/mappers/communityPhoto.xml" />
		<mapper resource="mybatis/mappers/restaurantPhoto.xml" />
		<mapper resource="mybatis/mappers/mountainPhoto.xml" />
		<mapper resource="mybatis/mappers/reviewPhoto.xml" />
		<mapper resource="mybatis/mappers/communityComment.xml" />
	</mappers>
	
</configuration>